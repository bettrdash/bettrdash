{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["lint", "^db#db:pre-build", "^build"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["db#db:generate"],
      "cache": false
    },
    "db:generate": {
      "outputs": ["dist/**"],
      "cache": false
    },
    "db:pre-build": {
      "outputs": ["dist/**"]
    }
  }
}
